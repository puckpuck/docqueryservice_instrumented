{
  "name": "docqueryservice-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Document Query Service React Frontend with World Bank API integration",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "audit": "yarn npm audit",
    "security-check": "yarn audit && yarn outdated",
    "docker:build": "docker build -t docqueryservice-frontend .",
    "docker:run": "docker run -p 3000:80 docqueryservice-frontend",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up -d --build",
    "docker:stop": "docker-compose down",
    "deploy:build": "yarn build && yarn docker:build",
    "deploy:local": "yarn deploy:build && yarn docker:compose:build"
  },
  "dependencies": {
    "@ant-design/icons": "5.3.7",
    "antd": "5.19.4",
    "axios": "1.12.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.25.1"
  },
  "devDependencies": {
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.8.0",
    "@vitejs/plugin-react": "4.3.1",
    "eslint": "9.10.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.12",
    "typescript": "5.4.5",
    "vite": "5.4.20"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "overrides": {
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0"
  },
  "resolutions": {
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0"
  },
  "packageManager": "yarn@4.10.2"
}
